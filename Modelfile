FROM gemma:2b

# Paramètres optimisés pour la vitesse
PARAMETER temperature 0.8
PARAMETER top_p 0.95
PARAMETER top_k 10
PARAMETER num_ctx 512
PARAMETER num_thread 5
PARAMETER repeat_penalty 1.1

# Système prompt optimisé pour des réponses rapides
SYSTEM """Tu es un assistant IA rapide et précis. Réponds en 2-3 phrases maximum. Va droit au but."""